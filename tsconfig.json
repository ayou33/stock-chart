{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "useDefineForClassFields": true,
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "downlevelIteration": true
  },
  "exclude": [
    "node_modules"
  ]
}
